  "description": "A github action that extracts labels from the PR that this build belongs to and make them available to other actions.",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asafb26/pr-labels-action-json.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Asafb26/pr-labels-action-json/issues"
  },
  "homepage": "https://github.com/Asafb26/pr-labels-action-json#readme",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "@vercel/ncc": "^0.25.1",
    "typescript": "^4.0.2"
  }
}
